// public/upload.ejs

<h1><i>S3</i> File Management Console</h1>
<hr />

<h3>File Uploaded</h3>
<form action="/uploadFile" enctype="multipart/form-data" method="post"> // 파일 업로드 폼
    <input type="file" name="attachment"> // 파일 선택 필드 (name="attachment" 중요)
    <button type="submit" class="btn btn-primary">Upload</button> // 제출 버튼
</form>
<hr />

<iframe name="targetURL" class="iframe-preview center" width="100%" height="500" style="border: none;" src="/list"> // S3 파일 목록을 보여줄 iframe
</iframe>